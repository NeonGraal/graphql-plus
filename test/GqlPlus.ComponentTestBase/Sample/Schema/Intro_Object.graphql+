output _TypeObject<$kind $parent $field $alternate> {
    : _ChildType<$kind $parent>
        typeParameters: _Described[]
        fields: $field[]
        alternates: $alternate[]
        allFields: _ObjectFor<$field>[]
        allAlternates: _ObjectFor<$alternate>[]
    }

dual _ObjDescribed<$base> {
        base: $base
        description: String
    | $base
    }

output _ObjType<$base> {
    | _BaseType<_TypeKind.Internal>
    | _TypeSimple
    | $base
    }

output _ObjBase {
        typeArguments: _ObjDescribed<_ObjArgument>[]
    | _TypeParameter
    }

output _ObjArgument {
    : _TypeRef<_TypeKind>
    | _TypeParameter
}

domain _TypeParameter { :_Identifier String }

output _Alternate<$base> {
      type: _ObjDescribed<$base>
      collections: _Collections[]
    }

output _ObjectFor<$for> {
    : $for
        object: _Identifier
    }

output _Field<$base> {
    : _Aliased
      type: _ObjDescribed<$base>
      modifiers: _Modifiers[]
    }
