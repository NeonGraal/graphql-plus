<article>
  {%- assign typ = type[1] %}
  {%- render 'heading', dec: typ, label: typ.typeKind -%}
  {%- case typ.typeKind %}
    {%- when "Domain" or "Enum" or "Union" %}
      {%- render "simple" with typ as simple %}
    {%- when "Dual" or "Input" or "Output" %}
      {%- render "object" with typ as object %}
    {%- else %}
      <i>Other type:</i> {{ typ.typeKind }}
  {%- endcase %}
</article>
