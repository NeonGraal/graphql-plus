<article>
  {%- assign cat = category[1] %}
  {%- render 'heading', dec: cat, label: "Category" -%}
  <div class="grid">
    <div>
      <sub>Output:</sub>
      {%- render "descr", name: cat.output.name, descr: cat.output.description -%}
      <sup> {{ cat.output.typeKind }} </sup>
    </div>
    <div>
      <sub>Resolution:</sub> {{ cat.resolution }}
    </div>
  </div>
</article>
