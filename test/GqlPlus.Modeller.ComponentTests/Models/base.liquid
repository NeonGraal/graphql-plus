{%- assign tag = base | tag -%}
{%- if tag == '_TypeParam' -%}
  ${{ base }}
{%- elsif base.typeKind == "Enum" -%}
  {{ base.name }}.{{ base.value }}
{%- else -%}
  {{ base.dual }}{{ base.input }}{{ base.output }}
  {%- if base.typeArgs -%}
    &lt; {% for arg in base.typeArgs -%}
      {%- render "base" with arg as base -%}
      {%- unless forloop.last %}, {% endunless -%}
    {%- endfor %} &gt;
  {%- endif -%}
{%- endif -%}